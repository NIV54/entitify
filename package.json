{
  "name": "entitify",
  "version": "0.0.1",
  "description": "blazing fast API creation",
  "author": "Niv Zelber",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/niv54/entitify"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build:prod": "tsc --build tsconfig.json",
    "clean:prod": "gulp clean:output",
    "prepare": "husky install",
    "prepack": "npm run build:prod",
    "postpublish": "npm run clean:prod",
    "publish": "lerna publish",
    "format": "prettier \"**/*.ts\" --ignore-path ./.prettierignore --write && git status",
    "lint:packages": "eslint --no-error-on-unmatched-pattern --cache 'packages/**/**.ts'"
  },
  "dependencies": {
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.3",
    "@nestjs/mapped-types": "^1.0.0",
    "@nestjs/testing": "^8.0.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "husky": "^7.0.2",
    "lerna": "^4.0.0",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.2",
    "typeorm": "^0.2.34",
    "typescript": "^4.4.2"
  }
}
